    "mockdate": "3.0.5",
    "mode-watcher": "0.5.0",
    "postcss": "8.4.49",
    "prettier": "3.4.1",
    "prettier-plugin-jsdoc": "1.3.0",
    "prettier-plugin-svelte": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "svelte": "5.4.0",
    "svelte-check": "4.1.0",
    "svelte-confetti": "2.2.0",
    "svelte-radix": "2.0.1",
