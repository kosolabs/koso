    "@playwright/test": "1.52.0",
    "@sveltejs/adapter-static": "3.0.8",
    "@sveltejs/kit": "2.20.8",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tailwindcss/postcss": "4.1.5",
    "@types/eslint": "9.6.1",
    "@types/node": "22.15.3",
    "@types/uuid": "10.0.0",
